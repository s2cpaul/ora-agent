# ==============================================================================
# ORA - AI Leadership Agent - Environment Template
# ==============================================================================
# Copy this file to .env and fill in your real credentials
# ==============================================================================

# ------------------------------------------------------------------------------
# SUPABASE (REQUIRED)
# ------------------------------------------------------------------------------
# Get from: https://supabase.com/dashboard → Your Project → Settings → API
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# ------------------------------------------------------------------------------
# OPTIONAL SERVICES
# ------------------------------------------------------------------------------

# Stripe Payments (Optional)
# VITE_STRIPE_PUBLIC_KEY=pk_test_your_key_here

# PayPal Payments (Optional)
# VITE_PAYPAL_CLIENT_ID=your_client_id_here
# VITE_PAYPAL_MODE=sandbox

# Google OAuth (Optional)
# VITE_GOOGLE_CLIENT_ID=your_client_id.apps.googleusercontent.com

# ------------------------------------------------------------------------------
# APPLICATION SETTINGS
# ------------------------------------------------------------------------------
VITE_APP_ENV=development
VITE_API_BASE_URL=http://localhost:5173
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_FEEDBACK_TRACKING=true
VITE_ENABLE_MULTI_AGENT=true
VITE_ENABLE_GOOGLE_OAUTH=false
VITE_DEBUG_MODE=false

# ------------------------------------------------------------------------------
# ADMIN
# ------------------------------------------------------------------------------
VITE_ADMIN_EMAILS=your-email@example.com

# ------------------------------------------------------------------------------
# RATE LIMITS
# ------------------------------------------------------------------------------
VITE_RATE_LIMIT_FREE=10
VITE_RATE_LIMIT_SOLO=50
VITE_RATE_LIMIT_BUDDY=100
VITE_RATE_LIMIT_TEAM=200
VITE_RATE_LIMIT_ENTERPRISE=1000